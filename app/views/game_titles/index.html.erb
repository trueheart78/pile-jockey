<p id="notice"><%= notice %></p>

<h1>Game Titles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_titles.each do |game_title| %>
      <tr>
        <td><%= game_title.name %></td>
        <td><%= link_to 'Show', game_title %></td>
        <td><%= link_to 'Edit', edit_game_title_path(game_title) %></td>
        <td><%= link_to 'Destroy', game_title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game Title', new_game_title_path %>
